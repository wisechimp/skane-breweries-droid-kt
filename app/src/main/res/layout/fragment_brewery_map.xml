<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    tools:context=".screens.main.ui.BreweryMapFragment">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/breweryMapView"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        mapbox:mapbox_cameraTargetLat="55.842"
        mapbox:mapbox_cameraTargetLng="13.386"
        mapbox:mapbox_renderTextureMode="true"
        mapbox:mapbox_cameraZoom="7"/>

</FrameLayout>